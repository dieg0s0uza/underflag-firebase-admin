{
  "name": "underflag-firebase-admin",
  "version": "1.0.0",
  "description": "firebase remote config provider for underflag",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "test": "jest",
    "prepublish": "npm run test && npm run build",
    "example": "npx ts-node examples/firebase-admin",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dieg0s0uza/underflag-firebase-admin.git"
  },
  "keywords": [
    "feature flag",
    "feature toggle",
    "remote config",
    "firebase"
  ],
  "author": "Diego Souza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dieg0s0uza/underflag-firebase-admin/issues"
  },
  "homepage": "https://github.com/dieg0s0uza/underflag-firebase-admin#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ]
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.8",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "firebase-admin": "^10.0.1",
    "underflag": "^1.0.0"
  }
}